    "autoload": {
        "psr-4": {
            "Palasthotel\\FirebaseNotifications\\": "classes"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/palasthotel/wp-components.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "kreait/firebase-php": "^5.11",
        "palasthotel/wp-components": "0.0.8"
    }
}
